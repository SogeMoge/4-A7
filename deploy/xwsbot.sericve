[Unit]
Description=Podman management service for xwsbot

[Service]
Type=oneshot
User=sogemoge
WorkingDirectory=/opt/4-A7/deploy
ExecStart=/opt/4-A7/deploy/podman-compose.sh up
ExecStop=/opt/4-A7/deploy/podman-compose.sh down
RemainAfterExit=yes
TimeoutStartSec=0
TimeoutSec=3600

[Install]
WantedBy=multi-user.target